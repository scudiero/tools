	[Is Undergraduate] isUndergraduate,
	[Is Graduate] isGraduate,
	[Is General Education] isGenEd,
	[Is New] isNew,
	[Is not New] isNotNew,
	[Is Edit/Change] isEdit,
	[Is not Edit/Change] isNotEdit,
	[Is Delete] isDelete,
	[Is not Delete] isNotDelete,
	[Is Inactivate] isInactivate,
	[Is not Inactivate] isNotInactivate,
	[Is Reactivate] isReactivate,
	[Is not Reactivate] isNotReactivate,
	END

